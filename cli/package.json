{
  "name": "deepdanci",
  "version": "0.1.0",
  "description": "CLI for adding DeepDanci UI components to your project",
  "bin": {
    "deepdanci": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
    "lint": "eslint \"src/**/*.ts\"",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "ui",
    "components",
    "cli"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.4.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "inquirer": "^8.2.4",
    "ora": "^5.4.1",
    "prompts": "^2.4.2",
    "execa": "^5.1.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.7.14",
    "eslint": "^8.23.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2"
  }
}
